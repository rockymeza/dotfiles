! Alt_L -> Super_L for the Model M
remove Mod1 = Alt_L
remove Mod4 = Super_L
keycode 64 = Super_L
add Mod4 = Super_L

clear Lock
! Caps_Lock -> Control_L
keycode 66 = Control_L
add Control = Control_L

! Shuffle (TypeMatrix) -> Control_L
keycode 102 = Control_L
add Control = Control_L

! Compose Key
keycode 107 = Multi_key

! Menu (TypeMatrix) -> Compose Key
